[req]
default_bits       = 4096
prompt             = no
default_md         = sha256
distinguished_name = req_distinguished_name
req_extensions     = req_ext

[req_distinguished_name]
C  = US
ST = CO
L  = Colorado Springs
O  = Client Organization
OU = Client Unit
CN = Client Common Name

[req_ext]
subjectAltName = @alt_names

[alt_names]
DNS.1 = client.GI-Bill.com
DNS.2 = localhost
IP.1  = 127.0.0.1
